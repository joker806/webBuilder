{% container Page %}
<div class="container">
	<header>
		{% slot header %}
	</header>

	<div class="container-fluid">
		<div class="row-fluid">
			<div class="span6">
				{% slot leftColumn %}
			</div>

			<div class="span6">
				{% slot rightColumn %}
			</div>
		</div>
		<div class="span12">
			{% slot content %}
		</div>
	</div>

	<hr>
	<footer>
		{% slot footer %}
	</footer>
</div>