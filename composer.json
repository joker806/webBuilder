{
    "name"    : "WebBuilder/WebBuilder",
    "version" : "0.1",
    
    "repositories": [{
        "type"    : "package",
        "package" : {
            "name"    : "sabberworm/PHP-CSS-Parser",
            "version" : "1.0.0-dev", 
            "source"  : {
            	"type"      : "git",
                "url"       : "https://github.com/sabberworm/PHP-CSS-Parser.git",
                "reference" : "master"
            }
        }
    }],
    
    "require" : {
        "php"             : ">=5.3",
        "twig/twig"       : "1.6.*",
        "imagine/Imagine" : "0.2.8",
        
        "sabberworm/PHP-CSS-Parser" : "1.0.0-dev"
    },
    
    "autoload" : {
        "psr-0" : {
        	"DemoCMS"    : "src/",
        	"ExtAdmin"   : "src/",
            "Inspirio"   : "src/",
            "WebBuilder" : "src/"
        },
        
        "classmap" : [
        	"vendor/sabberworm/PHP-CSS-Parser"
        ]
    }
}