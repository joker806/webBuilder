{% container WebPage %}
<!doctype html>
<!--[if lt IE 7]> <html class="no-js ie6 oldie" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js ie7 oldie" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js ie8 oldie" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

	<title>{% block pageTitle %}{{ structureItem.title }}{% endblock %}</title>
	<meta name="description" content="webBuilder test site">
	<meta name="author" content="Josef Martinec <joker806@gmail.com>">

	<meta name="viewport" content="width=device-width,initial-scale=1">

	<link rel="stylesheet" href="public/css/style.css">

	<script src="public/js/libs/modernizr_2.0/modernizr-2.0.min.js"></script>
	<script src="public/js/libs/respond/respond.min.js"></script>
	
	<style>/*
		.block, .slot { margin: 1px; padding: 16 3 3 3; }
		
		.block { border: 2px solid red; }
		.block > .name { background-color: red; }
		
		.slot { border: 2px solid lime; }
		.slot > .name { background-color: lime; }
		*/

		.block, .slot { margin: 0px; padding: 0px; }
		.block { border : 1px solid red; }
		.block > .name { display: none; }
		
		.slot { border : 0px; }
		.slot > .name { display: none; }
		
		.name { font-size: 9px; font-weight: bold; padding: 2px 5px; margin: -16 1 1 -3; float: left; }
		
		span.name > .data { float: right; display: none; background: #ddd; }
		span.name:hover > .data { display: block; }
		
		.content div { display: inline; }
	</style>
</head>
<body>
	{% slot masterContent %}

	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js"></script>
	<script>window.jQuery || document.write('<script src="js/libs/jquery-1.6.2.min.js"><\/script>')</script>
	
	<script src="js/script.js"></script>
	
	<!--[if lt IE 7 ]>
		<script src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.2/CFInstall.min.js"></script>
		<script>window.attachEvent("onload",function(){CFInstall.check({mode:"overlay"})})</script>
	<![endif]-->
</body>
</html>